server:
  port:8080
security:
  user:
    password:root
    name:root
eureka:
  instance:
    hostname: localhost
  client:
    registerWithEureka: false
    fetchRegistry: false
    serviceUrl: 
      defaultZone:http://${eureka.instance.hostname}:${server.port}/eureka/
spring:
  application: 
    name: com.yhem.yunmirror/eureka-server